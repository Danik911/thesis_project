# API Keys - Replace with your actual keys
CO_API_KEY="your-co-api-key"
OPENAI_API_KEY="your-openai-api-key"
QDRANT_API_KEY="your-qdrant-api-key"
QDRANT_URL="your-qdrant-url"
TAVILY_API_KEY="your-tavily-api-key"
ANTHROPIC_API_KEY="your-anthropic-api-key"
GITHUB_TOKEN="your-github-token"
GEMINI_API_KEY="your-gemini-api-key"
GOOGLE_CLOUD_PROJECT="your-google-cloud-project"
BRAVE_API_KEY="your-brave-api-key"
PERPLEXITY_API_KEY="your-perplexity-api-key"
PPLX_API_KEY="your-pplx-api-key"
ONE_SEARCH_API_KEY="your-one-search-api-key"

# Phoenix Observability Configuration
PHOENIX_HOST=localhost
PHOENIX_PORT=6006
PHOENIX_ENABLE_TRACING=true
PHOENIX_PROJECT_NAME=test_generation_thesis
PHOENIX_EXPERIMENT_NAME=multi_agent_workflow

# OpenTelemetry Configuration
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:6006/v1/traces
OTEL_SERVICE_NAME=test_generator

# Model Configuration
LLM_MODEL=gpt-4o-mini
WORKFLOW_TIMEOUT=1800
LLM_TIMEOUT=600
LLM_MAX_RETRIES=5
OPENAI_TIMEOUT=600

# Embedding Model Configuration (CRITICAL: Must be consistent across all components)
EMBEDDING_MODEL=text-embedding-3-small

# RAG Configuration
RAG_ONLY_MODE=true
RAG_CONFIDENCE_THRESHOLD=0.7
RAG_ENABLE_QUERY_EXPANSION=true
RAG_MAX_CHUNKS_PER_QUERY=10

# Document Processing
RAG_CHUNK_SIZE=1500
RAG_CHUNK_OVERLAP=200
RAG_MAX_DOCUMENT_PAGES=30
RAG_SUPPORTED_FORMATS=pdf,md,txt

# Vector Database
RAG_VECTOR_STORE_PATH=./lib/chroma_db
RAG_ENABLE_METADATA_FILTERING=true
RAG_CACHE_EMBEDDINGS=true
RAG_CACHE_DIR=./cache/rag

# Performance
RAG_QUERY_TIMEOUT=5000
RAG_BATCH_SIZE=50
RAG_ENABLE_PARALLEL_PROCESSING=true

# Advanced Features
RAG_ENABLE_GRAPHRAG=false
RAG_ENABLE_HYBRID_SEARCH=true

# RAG Extractor Model Configuration (Phase 1 Cost Optimization)
RAG_EXTRACTOR_MODEL=gpt-4.1-nano-2025-04-14
RAG_EXTRACTOR_TEMPERATURE=0.1
RAG_EXTRACTOR_MAX_TOKENS=500

# Project Specific
ENGINEER_NAME="Your Name"